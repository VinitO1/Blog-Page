<%- include("./partials/header") -%>
<div class="homePage">
	<h1>Welcome to My Daily Journal</h1>

	<p><%= homeStartingContent%></p>

	<% posts.forEach(function (post) { %>
	<h2><%= post.title %></h2>
	<p>
		<%= post.content.substring(0, 300) + "..." %>
		<a href="/posts/<%= post.title %>">Read More</a>
	</p>
</div>

<% }) %> <%- include("./partials/footer") -%>
